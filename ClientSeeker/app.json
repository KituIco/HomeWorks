{
  "expo": {
    "name": "ClientSeeker",
    "slug": "ClientSeeker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/HomeWorks-Icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/homeworks-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/HomeWorks-Icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.kituico.ClientSeeker"
    },
    "web": {
      "favicon": "./assets/HomeWorks-Icon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "db940bc9-9ad2-4e5c-82fb-56bbb14ecaec"
      }
    }
  }
}
